<!-- Modal -->
<div class="modal fade" id="taxResidentModal" tabindex="-1"
	role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title" id="exampleModalLabel">TAX Resident
					Details</h5>
				<button type="button" class="close" data-dismiss="modal"
					aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<div class="modal-body">
				<form>

					<div class="md-form form-sm mb-0">
						<input type="text" id="taxCountry" class="form-control form-control-sm mb-0"> <label for="taxCountry">Tax Country</label>
					</div>
					<div class="md-form form-sm mb-0">
					<input type="text" id="taxIdentype" class="form-control form-control-sm mb-0"> <label for="taxIdentype">Tax Identification Type</label>
					</div>

					<div class="md-form form-sm mb-3">
						<input type="text" id="taxIdenNumber" class="form-control form-control-sm mb-0"> <label for="taxIdenNumber">Tax Identification No</label>
					</div>
					
					<div class="md-form form-sm mb-0">
						<input type="date" id="taxtrcExpiryDate" class="form-control form-control-sm mb-0"> <label for="taxtrcExpiryDate">TRC Expiry Date</label>
					</div>
					
					<div class="md-form form-sm mb-0">
					<input type="text" id="taxaddressType" class="form-control form-control-sm mb-0"> <label for="taxaddressType">Tax Address Type</label>
					</div>
					<div class="md-form form-sm mb-0">
					<input type="text" id="taxAdd1" class="form-control form-control-sm mb-0"> <label for="taxAdd1">Address 1</label>
						
					</div>
					<div class="md-form form-sm mb-0">
					<input type="text" id="taxAdd2" class="form-control form-control-sm mb-0"> <label for="taxAdd2">Address 2</label>
						
						
					</div>
					
					
					<div class="md-form form-sm mb-0">
						<div class="col-6">
						<input type="text" id="taxCity" class="form-control form-control-sm mb-0"> <label for="taxCity">City</label>
						</div>
						<div class="col-6">
						<input type="text" id="taxState" class="form-control form-control-sm mb-0"> <label for="taxState">State</label>
						</div>
						</div>
					
					<div class="md-form form-sm mb-0">
						<div class="col-6">
						<input type="text" id="taxPostalCode" class="form-control form-control-sm mb-0"> <label for="taxPostalCode">Postal Code</label>
						</div>
						<div class="col-6">
						<input type="text" id="taxLandmark" class="form-control form-control-sm mb-0"> <label for="taxLandmark">Landmark</label>
						</div>
						</div>
					
					<div class="md-form form-sm mb-0">
						<div class="col-6">
						<input type="text" id="taxStdCodePr" class="form-control form-control-sm mb-0"> <label for="taxStdCodePr">Std Code primary</label>
					</div>
					
					<div class="col-6">
					<input type="text" id="taxTelNoPr" class="form-control form-control-sm mb-0"> <label for="taxTelNoPr">Primary Tel. No.</label>
					</div>
					</div>
					<div class="md-form form-sm mb-0">
					<div class="col-6">
					<input type="text" id="taxMobilePr" class="form-control form-control-sm mb-0"> <label for="taxMobilePr">Mobile No.</label>
					</div>
					<div class="col-6">
						<input type="text" id="taxStdOther" class="form-control form-control-sm mb-0"> <label for="taxStdOther">STD - Other</label>
					</div>
						
					</div>
					
				</form>

			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-sm btn-secondary">Reset</button>
				<button type="button" class="btn btn-sm btn-primary">Add Details</button>
			</div>
		</div>
	</div>
</div>